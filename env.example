# MSD Broken Links Analysis Actor - Environment Variables
# Copy this file to .env and modify as needed

# User Agent for HTTP requests
USER_AGENT=Mozilla/5.0 (compatible; MSD-BrokenLinks/1.0)

# Default timeout for link checking requests (in milliseconds)
DEFAULT_TIMEOUT=10000

# Default batch size for processing links
DEFAULT_BATCH_SIZE=5

# Maximum number of redirects to follow
MAX_REDIRECTS=5

# Whether to include external links in analysis
INCLUDE_EXTERNAL_LINKS=true

# Whether to include internal links in analysis
INCLUDE_INTERNAL_LINKS=true
